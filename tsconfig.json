{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/shared/assets/*"
      ],
      "rollup/parseAst": [
        "node_modules/rollup/dist/parseAst.d.ts"
      ]
    },
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    // "forceConsistentCasingInFileNames": true
  },
  "types": [
    "node",
    "vite/client",
    "vite-plugin-svgr/client"
  ],
}